<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatistest.custom.CustomMapper">
    <select id="getUserByName" parameterType="java.lang.String" resultType="com.example.mybatistest.biz.Users">
        SELECT * from users
        WHERE name = #{name}
    </select>
</mapper>